<!DOCTYPE html>
<html>
  <head>
    <title>OvertimeLog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= render 'shared/nav' %>
      <%= render 'shared/messages' %>
      <%= yield %>
    </div>
  </div>
</div> 

<%= js add_gritter(flash[:notice], title: "Overtime App Notification", sticky: false) %>

</body>
</html>
